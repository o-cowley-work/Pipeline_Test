@Library('test_library') _ 

node {
    runPipeline() 
}


def runPipeline() {
    stage {
		stage('Test library function') {

			steps {
				echo 'Doing a first thing'

				test_var.print_text()
				
			}
		}
		
		stage('Run test return library function') {
		    script {
                def a = test_var.return_string()

                echo a 
            }
		}
	}
}